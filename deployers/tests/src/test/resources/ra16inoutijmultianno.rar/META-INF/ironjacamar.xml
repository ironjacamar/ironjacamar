<ironjacamar xmlns="http://www.ironjacamar.org/schema"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://www.ironjacamar.org/schema 
             http://www.ironjacamar.org/schema/ironjacamar_1_0.xsd">

  <transaction-support>NoTransaction</transaction-support>
  
