<?xml version="1.0" encoding="UTF-8"?>

<resource-adapters xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../../../../main/resources/schema/resource-adapter_1_0.xsd">
	<resource-adapter>
		<archive>token</archive>
		<connection-definitions>
			<connection-definition class-name="token"
				jndi-name="token" enabled="true" pool-name="aSAPXcess">
				<recovery>
					<recover-plugin/>
				</recovery>
			</connection-definition>
		</connection-definitions>
		<admin-objects>
			<admin-object class-name="token" enabled="true"
				use-java-context="false">
				<config-property name="token" />
			</admin-object>
		</admin-objects>
	</resource-adapter>
</resource-adapters>